function checkSpeed(speed){
    if (speed <=80){
        return `good`}
    }else if(speed>80 && speed<140)
        {return(speed-80)/5 +``+`demerit points`

        }else{
            return `license suspended`
        }

//// Example usage////
let inputSpeed = parseInt(prompt("Enter speed :"));

//// Validate input////

if (!isNaN(inputSpeed)) {
    console.log("invalid entry.");
} else {
    checkSpeed(inputSpeed);
}
